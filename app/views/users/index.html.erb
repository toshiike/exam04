<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= link_to user_path(user) do %>
          ユーザ：<%= user.name %>
        <% end %>
        <%= profile_img(user) %>
      </br>
        <%= user.email %>
      </div>
  <% end %>
</div>
